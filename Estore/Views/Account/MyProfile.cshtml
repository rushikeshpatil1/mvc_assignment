@model Estore.Identity.ApplicationUser

@{
    ViewBag.Title = "MyProfile";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>My Profile</h2>
<table class="table table-bordered">
    <tr>
        <th>Username</th>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Mobile</th>
        <td>@Model.PhoneNumber</td>
    </tr>

</table>

<a href="/home/index">Back</a>

